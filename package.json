{
  "name": "mane-api",
  "version": "1.1.0",
  "private": true,
  "engines": {
    "node": "^20 || ^22"
  },
  "scripts": {
    "db:gen": "prisma generate --env-file=.env",
    "db:migrate": "prisma migrate dev --env-file=.env",
    "dev": "ts-node-dev --respawn --transpile-only --files src/index.ts",
    "build": "prisma generate && tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "start:migrate": "prisma migrate deploy && node dist/index.js",
    "prisma:migrate": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "lint": "echo \"add eslint if you want\"",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^6.16.3",
    "@types/compression": "^1.8.1",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.4.0",
    "helmet": "^7.1.0",
    "pino": "^9.5.0",
    "pino-http": "^10.3.0",
    "qrcode": "^1.5.4",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^9.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^22.7.5",
    "@types/uuid": "^9.0.7",
    "pino-pretty": "^13.1.1",
    "prisma": "^6.16.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.3"
  }
}
