{
  "name": "mane-api",
  "version": "1.1.6",
  "private": true,
  "engines": {
    "node": ">=18.18 <23"
  },
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only --files src/index.ts",

    "// local-only helpers": "------------------------------------------------",
    "db:gen": "node node_modules/prisma/build/index.js generate",
    "db:migrate": "node node_modules/prisma/build/index.js migrate dev",

    "// build/start (prod)": "------------------------------------------------",
    "prebuild": "node node_modules/prisma/build/index.js generate",
    "build": "node node_modules/typescript/lib/tsc.js -p tsconfig.json",
    "start": "node dist/index.js",
    "prisma:migrate": "node node_modules/prisma/build/index.js migrate deploy",

    "lint": "echo \"add eslint if you want\""
  },
  "dependencies": {
    "@prisma/client": "^6.16.3",
    "prisma": "^6.16.3",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.4.0",
    "helmet": "^7.1.0",
    "pino": "^9.5.0",
    "pino-http": "^10.3.0",
    "qrcode": "^1.5.4",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^9.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/compression": "^1.8.1",
    "@types/express": "^4.17.21",
    "@types/node": "^22.7.5",
    "@types/uuid": "^9.0.7",
    "pino-pretty": "^13.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.3"
  }
}
